{
    "name":           "rdlowrey/nbsock",
    "homepage":       "https://github.com/rdlowrey/nbsock",
    "description":    "Async socket tools built on the amp concurrency framework",
    "keywords": [
        "tcp",
        "sockets",
        "tls",
        "encryption",
        "async",
        "non-blocking",
        "amp"
    ],
    "license":        "MIT",
    "authors": [
        {
            "name": "Daniel Lowrey",
            "email": "rdlowrey@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5",
        "amphp/amp": "^1",
        "amphp/dns": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "4.8.x-dev"
    },
    "autoload": {
        "psr-4": {
            "Nbsock\\": "lib/"
        },
        "files": ["lib/functions.php"]
    },
    "minimum-stability": "dev",
    "autoload-dev": {
        "psr-4": {
            "Nbsock\\Test\\": "test/"
        }
    }
}
